<body>

    <header class="header-wrapper">
        <!-- Header Nav Start -->

        <!-- Header Nav End -->
        <div class="header-top bg-white ptb-30px d-xl-block d-none">
            <div class="container">
                <div class="row">
                    <div class="col-md-2">
                        <div class="logo">
                            <a href="index.html"><img class="img-responsive" src="FO/images/logo/logo.png"
                                    alt="logo.jpg" /></a>
                        </div>
                    </div>
                    <div class="col-md-10 align-self-center">
                        <div class="header-right-element d-flex">
                            <div class="search-element media-body">
                                <form class="d-flex" action="#">
                                    <div class="search-category">
                                        <select>
                                            <option value="0">All categories</option>

                                        </select>
                                    </div>
                                    <input type="text" placeholder="Enter your search key ... " />
                                    <button><i class="icon-magnifier"></i></button>
                                </form>
                            </div>

                            <!--Cart info Start -->
                            <div class="header-tools d-flex">
                                <div class="cart-info d-flex align-self-center">
                                    <a href="#offcanvas-wishlist" style="margin-left: 50px"
                                        class="heart offcanvas-toggle" data-number="3"><i class="icon-heart"></i></a>
                                    <a href="#offcanvas-cart" class="bag offcanvas-toggle" data-number="3"><i
                                            class="icon-bag"></i><span>$20.00</span></a>
                                </div>
                            </div>
                            <div style="margin-left: 50px">
                                <div class="phone">
                                    {% if app.user %}
                                    {#<a class="btn btn-danger " href="/logout">LOGOUT</a>#}
                                    <a class="btn btn-danger" href="{{ path('app_logout') }}">LOGOUT</a>

                                    {% else %}
                                    {#<a class="btn btn-success " href="/Register">Register</a>#}
                                    {#<a class="btn btn-success " href="/login">LOGIN</a>#}
                                    <a class="btn btn-success" href="{{ path('app_register') }}">Register</a>
                                    <a class="btn btn-success" href="{{ path('app_login') }}">LOGIN</a>

                                    {% endif %}
                                </div>
                            </div>
                        </div>

                        <!--Cart info End -->
                    </div>
                </div>
            </div>
        </div>
        <!-- Header Nav End -->
        <div class="header-menu bg-blue sticky-nav d-xl-block d-none padding-0px">
            <div class="container">
                <div class="row">
                    <div class="col-lg-3 custom-col-2">
                        <div class="header-menu-vertical">
                            <h4 class="menu-title">All Cattegories</h4>
                            <ul class="menu-content display-none">
                                <li>

                                    <ul class="submenu-item">
                                        <li><a href="#">Cables & Adapters</a></li>

                                    </ul>
                                </li>
                            </ul>
                            </ul>
                            <!-- menu content -->
                        </div>
                        <!-- header menu vertical -->
                    </div>
                    <div class="col-lg-9 custom-col-2">
                        <div class="header-horizontal-menu">
                            <ul class="menu-content">
                                <li class="active ">
                                    <a href="/">Home </a>

                                </li>
                                <li class="text-end"><a href="contact.html">Annonces</a></li>
                                <li class="text-end"><a href="contact.html">A propos</a></li>
                          
                                <li class="text-end"><a href="contact.html">contact Us</a></li>
                            </ul>

                        </div>
                        <!-- header horizontal menu -->

                    </div>
                </div>
                <!-- row -->
            </div>
            <!-- container -->
        </div>
        <!-- header menu -->
    </header>
   
   
    
     <div class="shop-category-area mt-30px">
        <div class="container">

            {% block body %}{% endblock %}
        </div>
    </div>

    
